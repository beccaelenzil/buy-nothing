<div class="card" style="width: 18rem;">
  <img src=<%= group.map %> class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= group.name %></h5>
    <p class="card-text"><%= group.description %>.</p>
    <a href=<%="#{group.id}/edit" %> class="btn btn-primary">Edit Group</a>
    <a href=<%="#{group.id}" %> class="btn btn-primary">View Group</a>
  </div>
</div>